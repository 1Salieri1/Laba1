using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Laba12
{
    class Program
    {
        static void Main(string[] args)
        {
            A clasA = new A();

            Console.WriteLine(clasA.c);
            Console.ReadKey();
        }
    }
    class A
    {
        private int a = 9;
        private int b = 3;

        public int c 
        {
            get { return a *= b++; }
            set { --value; }
        }
    }
}
